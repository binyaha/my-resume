  <h2 class = "center">[施工中]</h2>
  <br>
  <div class = "row">
    <%= render partial: "shared/user_profile", locals: {obj: @user} %>
    
    <div class = "col-xs-8">
      <% @contacks.each do |contack| %>
        <li>
          <%= contack.sentence %>
          <%= link_to "編輯", edit_user_contack_path(@user, contack) %>
          <%= link_to "刪除", user_contack_path(@user, contack), method: :delete %>
          </li>
      <% end %>
    </div>
  
    <%= link_to "Say", new_user_contack_path(@user,@contack) %>

  </div>